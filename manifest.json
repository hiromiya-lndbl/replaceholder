{
    "manifest_version": 2,
    "name": "Replaceholder",
    "description": "Replace placeholders with date and time as you type.",
    "version": "1.0",
    "author": "hiromiya-lndbl",
    "applications": {
        "gecko": {
            "id": "replaceholder@lndbl.com",
            "strict_min_version": "78.0"
        }
    },
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "compose_action": {
        "default_title": "Replaceholder"
    },
    "permissions": [
        "compose"
    ]
}